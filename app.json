{
  "expo": {
    "name": "shoot-doori-fe",
    "slug": "shoot-doori-fe",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo_without_background.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "shoot-doori-fe",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/splash.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/logo_without_background.png"
    },
    "plugins": ["expo-router"]
  }
}
